# Remove the line below if you want to inherit .editorconfig settings from higher directories
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8-bom
trim_trailing_whitespace = false
insert_final_newline = false

[*.{js,ts,vue,json,cake,config,xml,csproj,mdx}]
indent_size = 2
charset = utf-8